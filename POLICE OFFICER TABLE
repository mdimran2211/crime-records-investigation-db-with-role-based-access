CREATE TABLE Police_Officer (
    Officer_ID INT PRIMARY KEY AUTO_INCREMENT,
    Name VARCHAR(100) NOT NULL,
    Officer_Rank VARCHAR(50) NOT NULL,
    Station_ID INT,
    Contact_Number VARCHAR(15)
);
